#include <ctype.h>
#include <cs50.h>
#include <stdio.h>

int main(void)
{
    int n = get_int("Number: ");
    if ((n >= 1) && (n <= 12))
    {
        if ((n == 1) || (n == 2) || (n == 12))
        {
            printf("Winter\n");
        }
        if ((n >= 3) && (n <= 5))
        {
            printf("Spring\n");
        }
        if ((n >= 6) && (n <= 8))
        {
            printf("Summer\n");
        }
        if ((n >= 9) && (n <= 11))
        {
            printf("Autumn\n");
        }
    }
    else
    {
        printf("Number must be from 1 to 12\n");
    }
}
